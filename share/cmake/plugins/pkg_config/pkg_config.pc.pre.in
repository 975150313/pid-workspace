global=@_PKG_CONFIG_WORKSPACE_GLOBAL_PATH_@
prefix=${global}/@_PKG_CONFIG_PACKAGE_PREFIX_@
libdir=${prefix}/lib
bindir=${prefix}/bin
includedir=${prefix}/include

Name: @_PKG_CONFIG_COMPONENT_NAME_@
